{
  "scripts": {
    "build": "cross-env webpack -p --mode=production --hide-modules --progress --color --display=minimal",
    "start": "cross-env NODE_ENV=dev webpack-dev-server --mode=development --color",
    "hello": "cross-env npm install && npm run start",
    "format-js": "cross-env prettier \"app/src/**/*.@(js)\" --write",
    "format-scss": "cross-env prettier \"app/src/**/*.@(scss)\" --parser scss --write",
    "pre-precommit": "cross-env pretty-quick --staged",
    "watch": "cross-env webpack -w --mode=development --progress --colors --hide-modules --display=minimal"
  },
  "lint-staged": {
    "*.js": [
      "npm run format-js"
    ],
    "*.scss": [
      "npm run format-scss"
    ]
  },
  "author": "Adrien Zaganelli <adrienzaganelli@gmail.com> (http://adrienzaganelli.com/)",
  "devDependencies": {
    "autoprefixer": "^9.7.6",
    "babel-core": "^6.26.3",
    "babel-eslint": "^8.2.3",
    "babel-loader": "^7.1.4",
    "babel-plugin-syntax-dynamic-import": "^6.18.0",
    "babel-plugin-transform-object-rest-spread": "^6.26.0",
    "babel-preset-env": "^1.7.0",
    "clean-webpack-plugin": "^0.1.19",
    "compression-webpack-plugin": "^1.1.11",
    "copy-webpack-plugin": "^5.1.1",
    "critters-webpack-plugin": "^2.5.0",
    "cross-env": "^5.2.1",
    "css-loader": "^1.0.0",
    "eslint": "^4.19.1",
    "eslint-loader": "^2.2.1",
    "glslify-loader": "^2.0.0",
    "hard-source-webpack-plugin": "^0.13.1",
    "html-webpack-plugin": "^3.2.0",
    "husky": "^0.14.3",
    "imagemin-mozjpeg": "^8.0.0",
    "imagemin-webp": "^5.1.0",
    "imagemin-webpack-plugin": "^2.4.2",
    "ip": "^1.1.5",
    "mini-css-extract-plugin": "^0.4.0",
    "node-sass": "^4.13.1",
    "offline-plugin": "^5.0.7",
    "postcss-loader": "^3.0.0",
    "prettier": "^1.19.1",
    "pretty-quick": "^1.11.1",
    "raw-loader": "^3.1.0",
    "sass-loader": "^7.3.1",
    "style-loader": "^0.22.1",
    "vue-loader": "^15.9.1",
    "vue-template-compiler": "^2.6.11",
    "webpack": "^4.42.1",
    "webpack-build-notifier": "^0.1.32",
    "webpack-cli": "^3.3.11",
    "webpack-dev-server": "^3.10.3",
    "webpack-manifest-plugin": "^2.2.0",
    "webpack-pwa-manifest": "^3.6.2",
    "webpackbar": "^4.0.0"
  },
  "dependencies": {
    "vue": "^2.6.11"
  }
}
