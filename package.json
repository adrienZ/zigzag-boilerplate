{
  "scripts": {
    "build": "CLEAR_DIST=true webpack -p --hide-modules --cache",
    "start": "NODE_ENV=dev webpack-dev-server --color",
    "open": "code . || open . -a atom",
    "hello":
      "(yarn install || npm i) && (yarn start --open || npm run start --open)",
    "deploy": "yarn build && gdd --branch gh-pages --src dist --remote origin",
    "prettier": "prettier \"app/src/**/*.@(js|scss)\" --write",
    "precommit": "pretty-quick --staged",
    "wp":
      "NODE_ENV=dev webpack -w --progress --colors --hide-modules --cache --bail --display=minimal"
  },
  "author":
    "Adrien Zaganelli <adrienzaganelli@gmail.com> (http://adrienzaganelli.com/)",
  "devDependencies": {
    "@hswolff/git-directory-deploy": "^0.6.1",
    "babel-core": "^6.24.0",
    "babel-eslint": "^8.2.2",
    "babel-loader": "^7.1.2",
    "babel-preset-es2015": "^6.24.0",
    "clean-webpack-plugin": "^0.1.16",
    "css-loader": "^0.28.7",
    "ejs-loader": "^0.3.1",
    "eslint": "^4.6.1",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.7.0",
    "extract-text-webpack-plugin": "^3.0.0",
    "favicons-webpack-plugin": "^0.0.7",
    "file-loader": "^1.1.6",
    "hard-source-webpack-plugin": "^0.6.4",
    "html-webpack-plugin": "^2.30.1",
    "husky": "^0.14.3",
    "image-webpack-loader": "^4.2.0",
    "ip": "^1.1.5",
    "node-sass": "^4.8.3",
    "path": "^0.12.7",
    "postcss-loader": "^2.0.6",
    "prettier": "1.11.1",
    "pretty-quick": "^1.4.1",
    "sass-loader": "^6.0.6",
    "style-loader": "^0.18.2",
    "url-loader": "^1.0.1",
    "webpack": "^3.6.0",
    "webpack-dev-server": "^2.8.2",
    "webpack-manifest-plugin": "^1.3.1"
  }
}
